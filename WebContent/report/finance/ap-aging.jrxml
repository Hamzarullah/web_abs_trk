						-- AND fin_general_journal_detail.headerCode NOT IN (SELECT fin_supplier_invoice.PaymentVoucherNo FROM fin_supplier_invoice WHERE fin_supplier_invoice.PaymentVoucherNo <> '')
					GROUP BY fin_general_journal_detail.DocumentNo
